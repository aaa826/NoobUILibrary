-- Scripts:

local function BVLDH_fake_script() -- Toggle.LocalScript 
	local script = Instance.new('LocalScript', Toggle)

	local toggled = true
	local debounce = false
	
	script.Parent.toggle.Circle.MouseButton1Click:Connect(function()
		if debounce == false then
			if toggled == true then
				debounce = true
				game:GetService("TweenService"):Create(script.Parent.toggle.Circle, TweenInfo.new(0.25), {Position = UDim2.new(0.542, 0, 0, 0)}):Play()
				game:GetService("TweenService"):Create(script.Parent.toggle, TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(68, 206, 0)}):Play()
				wait(0.25)
				debounce = false
				toggled = false
			elseif toggled == false then
				game:GetService("TweenService"):Create(script.Parent.toggle.Circle, TweenInfo.new(0.25), {Position = UDim2.new(-0, 0, 0, 0)}):Play()
				game:GetService("TweenService"):Create(script.Parent.toggle, TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(0, 203, 203)}):Play()
				wait(0.25)
				debounce = false
				toggled = true 
			
			end
		end
	end)
end
	coroutine.wrap(BVLDH_fake_script)()
	
	
	
